{
  "config": {
    "version": "1.0.0",
    "date": "2024-02-23",
    "url": "https://github.com/proyect/repo/folder",
    "branch": "branch",
    "logtype": "order"
  },
  "servers": [
    {
      "server_name": "customname_key_log",
      "logger_path": "/github/folder",
      "proxy": "http://proxy:8080/",
      "type": "tomcat/docker",
      "tomcat_folder": "/absolute/path/folder",
      "docker_name": "docker.xxx.com/xxx/xxx",
      "url": "https://url",
      "user": "user",
      "backups": "/absolute/path/folder",
      "keyfile": "/absoultepath/.ssh/id_rsa",
      "host": "hostname.com"
    }
  ],
  "actions": [
    {
      "type": "github_update",
      "description": "---------------------GITHUB-UPDATE---------------------",
      "servers": ["customname_key_log"]
    },
    {
      "type": "backups",
      "description": "---------------------BACKUPS---------------------(custom header text)",
      "servers": ["customname_key_log"]
    },
    {
      "type": "monit",
      "description": "---------------------MONIT-STATUS---------------------(custom header text)",
      "servers": ["customname_key_log"]
    },
    {
      "type": "analytics",
      "description": "---------------------ANALYTICS---------------------(custom header text)",
      "servers": ["customname_key_log"]
    },
    {
      "type": "cloning",
      "description": "---------------------Cloning---------------------(custom header text)",
      "servers": ["customname_key_log"]
    },

    {
      "type": "custom",
      "description": "---------------------DISK-SPACE---------------------(custom header text)",
      "command": "df -h",
      "servers": ["customname_key_log"]
    }
  ]
}
